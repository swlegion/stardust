<Defaults>
  <Text color="white" />
  <Button color="black" textColor="white" />
  <Button class="loadTable" onClick="Global/loadTable" />
  <Button class="loadList" onClick="Global/loadList" />
</Defaults>

<VerticalLayout
  height="320"
  width="80"
  rectAlignment="UpperRight"
  offsetXY="-10 -50"
>
  <VerticalLayout
    height="100"
    width="80"
  >
    <Text>Table</Text>
    <Button onClick="clearTable()">
      Clear
    </Button>
    <Button onClick="loadTable(3x3)">
      3x3
    </Button>
    <Button onClick="loadTable(6x3)">
      6x3
    </Button>
    <Button onClick="loadTable(6x4)">
      6x4
    </Button>
  </VerticalLayout>
  <VerticalLayout
    id="loadLists"
    height="100"
    width="80"
  >
    <Text>Lists</Text>
    <Button onClick="loadListByDemo">
      Demo
    </Button>
    <Button onClick="loadListByBuild" interactable="false">
      Build
    </Button>
    <Button onClick="loadListByImport" interactable="false">
      Load
    </Button>
  </VerticalLayout>
</VerticalLayout>

<Panel
  height="36"
  width="200"
  rectAlignment="UpperLeft"
  offsetXy="55 -8"
  color="black"
>
  <Text>Stardust Alpha</Text>
</Panel>
