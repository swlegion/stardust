{
  "name": "stardust",
  "version": "0.1.0",
  "description": "A new TTS mod.",
  "scripts": {
    "build": "node_modules/.bin/ts-node ./scripts/build.ts --skip-linking",
    "doctor": "node_modules/.bin/ts-node ./scripts/doctor.ts",
    "extract": "node_modules/.bin/ts-node ./scripts/extract.ts",
    "fix": "node_modules/.bin/ts-node ./scripts/fix.ts",
    "lint": "node_modules/.bin/ts-node ./scripts/lint.ts",
    "presubmit": "npm install && npm run lint && npm test",
    "test": "jest",
    "watch": "nodemon ./scripts/build.ts --watch src --watch lib --ext js,ts,xml,json,ttslua"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swlegion/stardust.git"
  },
  "author": "SWL Dev Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swlegion/stardust/issues"
  },
  "homepage": "https://github.com/swlegion/stardust",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^13.9.5",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "ajv-cli": "^3.1.0",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "jest": "^25.2.4",
    "mock-fs": "^4.11.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "sanitize-filename": "^1.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
